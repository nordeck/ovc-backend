<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.15.xsd">

    <property name="NOW" value="now()" dbms="mysql,h2"/>
    <property name="NOW" value="CURRENT_TIMESTAMP" dbms="postgresql"/>
    <property name="NOW" value="sysdate" dbms="oracle"/>

    <changeSet author="wagner.wutzke" id="1700574778670-1">
        <addColumn schemaName= "public" tableName="meeting">
            <column name="static_room" type="BOOLEAN" defaultValueBoolean="false" />
            <column name="last_visit_date" type="DATE" valueDate="${NOW}"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>